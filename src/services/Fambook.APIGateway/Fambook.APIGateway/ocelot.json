{
  "ReRoutes": [
    {
      "DownstreamPathTemplate": "/user/authenticate",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "fambook-authservice",
          "Port": 80
        }
      ],
      "UpstreamPathTemplate": "/api/user/authenticate",
      "UpstreamHttpMethod": [ "Post" ]
    },
    {
      "DownstreamPathTemplate": "/user/create",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "fambook-userservice",
          "Port": 80
        }
      ],
      "UpstreamPathTemplate": "/api/user/create"
    },
    {
      "DownstreamPathTemplate": "/user",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "fambook-userservice",
          "Port": 80
        }
      ],
      "UpstreamPathTemplate": "/api/user",
      "AuthenticationOptions": {
        "AuthenticationProviderKey": "AuthKey",
        "AllowedScopes": []
      }
    }
  ],
  "GlobalConfiguration": {
    "BaseUrl": "https://localhost:51000"
  }
}